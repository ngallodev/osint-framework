# Frontend Environment Variables
# Copy this file to .env.local and update with your local values
# The .env.local file is gitignored and not committed to the repository

# GraphQL API endpoint (leave as /graphql to use same-origin proxy)
VITE_REACT_APP_API_URL=/graphql

# Optional: Debug mode (set to true to enable verbose logging)
VITE_DEBUG_MODE=false

# Optional: Request timeout in milliseconds
VITE_REQUEST_TIMEOUT=30000

# VITE_AUTH0_DOMAIN=<YourAuth0Domain>

# VITE_AUTH0_CLIENT_ID=<YourAuth0ClientID>

# VITE_AUTH0_AUDIENCE=<YourAuth0Audience>
